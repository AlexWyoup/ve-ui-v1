<% content_for :content_settings do %>
<% provide(:title, @user.first_name) %>

<% if @user.organization %>
<%= image_tag @user.organization.logo %>
<br></br>
<ol class="organization">
	<%= render @user.organization %>
	
</ol>

<% else %>
<%= image_tag "blue.png", :alt => "Megam" %>
<div class="alert alert-warning">
	<span class="label label-important">!</span>
	<strong>Warning!</strong> Your account isn't complete. Kindly complete your profile before we could proceed further.
</div>
<button type="submit" class="btn btn-large btn-warning">
	Edit
</button>
<% end %>
<hr>
</hr>
Cloud Applications
<% end %>
<%= render :template => customizations_show_path %>

