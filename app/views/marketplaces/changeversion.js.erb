var selected_entity = '#version_view';
<% if @type == "APP" %>
var to_insert_content = "<%= escape_javascript(render :partial => 'marketplaces/app_view', :locals => {:version => @version, :mkp => @mkp}) %>";
<% elsif @type == "SERVICE" %>
var to_insert_content = "<%= escape_javascript(render :partial => 'marketplaces/app_boilers_view', :locals => {:version => @version, :mkp => @mkp}) %>";
<% elsif @type == "INSTANCE" %>
var to_insert_content = "<%= escape_javascript(render :partial => 'marketplaces/instance_view', :locals => {:version => @version, :mkp => @mkp}) %>";
<% else %>
var to_insert_content = "<%= escape_javascript(render :partial => 'marketplaces/docker_view', :locals => {:version => @version, :mkp => @mkp}) %>";
<% end %>
insertAt(selected_entity, to_insert_content);
