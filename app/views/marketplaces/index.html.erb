<div id="breadcrumb">
	<%= breadcrumbs.render(:format => :inline,:separator => '') %>
</div>
<div class="container">
	<div class="row">
		<div class="col-xs-11 col-lg-11 col-md-11"  id="category_view_value">			
			<%= render :partial => "marketplaces/category_view_form" , :locals => {:category => "All", :mkp_collection => @mkp_collection, :categories => @categories}%>
		</div>
	</div>
</div>

<% end %>
<%= render :template => users_show_path %>
