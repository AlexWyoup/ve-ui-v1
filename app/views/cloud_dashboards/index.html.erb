<% content_for :content_settings do %>
<% provide(:title, 'Dashboard') %>

<%= render :partial => "users/header" , :locals => {:user => current_user, :head => "Dashboard"}%>
<div id="breadcrumb">
	<%= breadcrumbs.render(:format => :inline,:separator => '') %>
</div>
<br/>

<div class="container">
	<%= render :partial => "cloud_dashboards/dash_index_page", :locals => {:apps_distrib => @apps_distrib}  %>
</div>
<div id="email_verify_popup" class="modal fade" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
		<div class="modal-dialog">
			<div class="modal-content">
				<div class="modal-header">
					<button type="button" class="close" data-dismiss="modal" aria-hidden="true">
						Ã—
					</button>
					<h3 id="myModalLabel">Email Verification</h3>
				</div>
				<div class="modal-body">
					<div class="alert alert-success">

						<div id="msg">
							Email has been sent to <h5><%= current_user.email %></h5> for verification.
							Please check your email and follow the instructions.
						</div>
					</div>
				</div>
				<div class="modal-footer">
					<button class="btn btn-default" data-dismiss="modal" aria-hidden="true">
						Ok
					</button>
				</div>
			</div>
		</div>
	</div>

<% end %>

<%= render :template => users_show_path %>
