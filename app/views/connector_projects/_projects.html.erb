<% @connector_project = ConnectorProject.all %>
<table class="table table-condensed table-striped">
	<thead>
		<tr>
			<th>Id</th>
			<th>When ?</th>
			<th>Description</th>
		</tr>
	</thead>
	<tbody>
	<% @connector_project.each do |connector_project_el| %>
	<tr id=<%= dom_id(connector_project_el) %>>

		<td> <%= connector_project_el.id %> </td>
		<td> <%= connector_project_el.created_at %> </td>

		<td> <%= connector_project_el.description %> </td>

	</tr>
	<% end %>
	</tbody>

</table>
