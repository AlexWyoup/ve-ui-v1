<div class="tabs" id="overview"></div>

<script type="text/javascript">
  (function ($) {
    $.AppName = "<%= @logname %>";
    $.MonitC = "http://" + $.AppName + ":8080/api/v1.3/containers";
    $.MonitM = "http://" + $.AppName + ":8080/api/v1.3/machine";
    $.SocketURL = "ws://<%= Ind.logs %>";
  })($);


  var serverSocket = new WebSocket($.SocketURL)

  $('.socket_disconnect').click(function () {
    serverSocket.close();
  });

  //replace host and mhost to $.MonitC and $.MonitM
  React.render(React.createElement(Overview, {
    google: google,
    socket: serverSocket,
    name: $.AppName,
    graphName: "LineChart",
    host: $.MonitC,
    mhost: $.MonitM
  }), document.getElementById('overview'));
</script>
