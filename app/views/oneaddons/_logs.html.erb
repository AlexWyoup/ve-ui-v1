
<div class="rt-box col-xs-12 col-sm-8 col-md-9 col-lg-9 col-xl-9" ng-controller="LogCtrl">

        <div class="row c_pading-b15 settings_top">
		<div class="margin_15">
			<div class="col-xs-9 col-sm-6 col-md-7">
				<h3> Logs </h3>
			</div>
			<%= render :partial => "users/topright_icons" %>
		</div>
	</div>


<a title="" href="" ng-click="sendmessage('logloader','<%= assembly_name %>')"><strong><%= assembly_name %></strong></a>

<input type="hidden" ng-model="source" />

                <div class="col-sm-9 col-md-9 col-lg-9">
			<div class="panel panel-primary" >				
				<div class="panel-heading" cg-busy="{promise:promise, message:message, backdrop:backdrop, delay:delay, minDuration:minDuration}">
					<div class="row">
						<label class="col-sm-7 col-md-7 col-lg-8 control-label">{{bookName}}</label>
						<!--<div class="col-sm-4 col-md-4 col-lg-4">
						<input type="search" ng-model="q" placeholder="filter message..." class="form-control input-sm"/>
						</div> -->
						<div class="col-sm-5 col-md-5 col-lg-4">
							<span class="pull-right"><a href="http://monitor.megam.co.in/kibana/#/dashboard/file/{{bookName}}.json" target="_blank" class="btn btn-pink btn-xs"><strong>Offline Log : {{bookName}} </strong></a></span>
						</div>
					</div>
				</div>
				<div class="panel-body nopadding" style="height:400px; overflow-y: scroll;">
					<table class="table table-bordered table-striped table-hover" >
						<thead>
							<tr>
								<th class="col-md-3">Timestamp <!-- <span class="label label-default">{{logTimezone }}</span>--></th>
								<th class="col-md-9">Message</th>
							</tr>
						</thead>
						<tbody>
							<tr ng-repeat="log in logs | orderBy:predicate:!reverse | slice:0:100">
								<td>
								<p>
									<small> {{ log.timestamp | date:'d MMM yy, hh:mm a Z' }}</small>
								</p></td>
								<td>
								<p>
									<small>{{log.message }}</small>
								</p></td>
							</tr>
						</tbody>
					</table>
				</div>
			</div>
		</div>

</div>





