<% content_for :content_settings do %>
<% provide(:title, 'cloud_books') %>
<div id="content-header">
	<h1>Cloud Books</h1>
	<div class="btn-group">
		<a class="btn btn-large tip-bottom" title="Manage Files"><i class="icon-file"></i></a>
		<a class="btn btn-large tip-bottom" title="Manage Users"><i class="icon-user"></i></a>
		<a class="btn btn-large tip-bottom" title="Manage Comments"><i class="icon-comment"></i><span class="label label-important">5</span></a>
		<a class="btn btn-large tip-bottom" title="Manage Orders"><i class="icon-shopping-cart"></i></a>
	</div>
</div>
<%= render_breadcrumbs %>

<div class="container-fluid">
	<div class="row-fluid">
		<table class="table table-bordered">
			<br/>
			<tbody>
				<tr class="success" >
					<td> <%= link_to predefs_path, :class => "btn btn-large btn-success btn-block", :title => "Icon Title" do %>  Rails<% end %> </td>
					<td> <%= link_to predefs_path, :class => "btn btn-large btn-success btn-block", :title => "Icon Title" do %>  Java<% end %> </td>
					<td> <%= link_to predefs_path, :class => "btn btn-large btn-success btn-block", :title => "Icon Title" do %>  Play<% end %> </td>
				</tr>
				<tr class="success">
					<td> <%= link_to predefs_path, :class => "btn btn-large btn-success btn-block", :title => "Icon Title" do %>  Akka<% end %> </td>
					<td> <%= link_to predefs_path, :class => "btn btn-large btn-success btn-block", :title => "Icon Title" do %>  Nodejs<% end %> </td>
				</tr>
			</tbody>
		</table>
	</div>
	<div class="row-fluid">
		<div class="widget-box">
			<div class="widget-title">
				<span class="icon"><i class="icon-time"></i></span>
				<h5>Cloud Identity predef's</h5>
			</div>
			<div class="widget-content nopadding">
				<table class="table table-striped table-bordered">
					<tbody>
						<tr>
							<td class="taskDesc"><i class="icon-info-sign"></i> SCM</td>
							<td class="taskStatus"><span class="full_text"><%= text_field :scm, :class => 'required', placeholder: "scm" %></span></td>
							<td class="taskOptions"></td>
						</tr>
						<tr>
							<td class="taskDesc"><i class="icon-info-sign"></i> DB</td>
							<td class="taskStatus"><span class="full_text"><%= text_field :db, :class => 'text_field required', placeholder: "db" %></span></td>
							<td class="taskOptions">
							<div class="post_info">
								<div class="slideThree">
									<input type="checkbox" value="db_status" id="db" name="db_status" />
									<label for="db"></label>
								</div>
							</div></td>
						</tr>
						<tr>
							<td class="taskDesc"><i class="icon-info-sign"></i>Queue</td>
							<td class="taskStatus"><span class="full_text"><%= text_field :queue, :class => 'text_field required', placeholder: "queue" %></span></td>
							<td class="taskOptions">
							<div class="post_info">
								<div class="slideThree">
									<input type="checkbox" value="queue_name" id="queue" name="queue" />
									<label for="queue"></label>
								</div>
							</div></td>
						</tr>

					</tbody>
				</table>
			</div>
		</div>
	</div>
</div>
<% end %>
<%= render :template => users_show_path %>

