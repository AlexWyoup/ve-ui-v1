<div class="rt-box col-xs-12 col-sm-9 col-md-10 c_bg_white c_height_5">

    <%= render :partial => "users/topright_icons", :locals => {:title_name => "Your Files", :upload_button => "true"} %>

    <!--	<%= react_component('Show' )  %> -->
    <div class="col-xs-12 col-sm-9 col-md-9 br_grey">
        <div class="row">
            <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12 col-xl-12">
                <table class="table yfiles-tb tb_upload objectPageList">
                    <tbody>
                        <% @objects[:objects].each do |obj| %>
                        <tr class="del-up-cover">
                            <!--<td width="5%">
                            <div class="checker">
                                <span class="">
                                    <input type="checkbox">
                                </span>
                            </div></td>-->
                            <td class="td-left objectListTd" id="<%= obj[:object_name] %>"><i id="objectFormat" class="pop_icon"></i><b><%= obj[:object_name].split("/").last %></b><i> - <%= obj[:size] %></i></td>
                            <td style="padding-top:17px"> <%= link_to obj[:download_url] do %> <i class="c_icon-d margin_15  pull-right"></i> <% end %> </td>
                            <td style="padding-top:15px"> <%= link_to bucketkolke_path( :id => obj[:object_name] ), method: :DELETE, :remote => true do %> <i class="c_icon-trash margin_15 pull-right"></i> <% end %> </td>
                        </tr>
                        <% end %>
                    </tbody>
                </table>
            </div>
        </div>
    </div>
    <%= render :partial => "bucketkolkes/usage", :locals => {:objects => @objects, :page => 'show'} %>
</div>
<%= render :partial => "buckets/upload_modal", :locals => {:bucket_name => @bucket_name} %>
<script src="assets/megam.storages.js" />
