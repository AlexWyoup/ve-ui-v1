<ul>
  <li>
    <%= link_to catalogs_path(:id => id, :name => name, :command => Assemblies::START, :cattype => cattype), :date => {:remote => true }, :method => :post do %>
      <i class="glyphicon glyphicon-play-circle"></i>Start <%= cattype %>
    <% end %>
  </li>
  <li>
    <%= link_to catalogs_path(:id => id, :name => name, :command => Assemblies::STOP, :cattype => cattype), :date => {:remote => true } do %>
      <i class="glyphicon glyphicon-record"></i>Stop <%= cattype %>
    <% end %>
  </li>
  <li>
    <%= link_to catalogs_path(:id => id, :name => name, :command => Assemblies::RESTART, :cattype => cattype), :date => {:remote => true } do %>
      <i class="glyphicon glyphicon-refresh"></i>Restart <%= cattype %>
    <% end %>
  </li>
  <li>
    <%= link_to catalogs_path(:id => id, :name => name, :command => Assemblies::DELETE, :cattype => cattype), :date => {:remote => true } do %>
      <i class="glyphicon glyphicon-trash"></i>Delete <%= cattype %>
    <% end %>
  </li>
</ul>
