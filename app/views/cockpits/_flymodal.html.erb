<!-- CATALOG DELETE MODAL -->
<div class="modal fade app_exist" id="cat_delete" tabindex="-1" role="basic" aria-hidden="true">
  <div class="modal-dialog">
    <%= form_tag catalogs_path, multipart: true, :novalidate => 'novalidate',  :method => 'post', :remote => true  do %>
      <div class="modal-content c_modal-content">
        <div class="modal-header">
          <button type="button" class="close" data-dismiss="modal" aria-hidden="true"></button>
          <h4 class="modal-title">Delete <%= cattype %></h4>
        </div>
        <div class="modal-body text-center">
          <span id="delete_asm_id"></span>
          <span id="delete_asm_name"></span>
          <%= hidden_field_tag 'command', nil, placeholder: "", :class =>"form-control", :value => Assemblies::DELETE  %>
          Are you sure you want to delete the <%= cattype.downcase %> ?
			</div>
        <div class="modal-footer c_modal-footer">
          <button type="button" class="btn default" data-dismiss="modal">
            Cancel
				</button>
          <%= submit_tag "Confirm", :class => "btn btn-danger", data: { disable_with: "Please wait..." } %>
        </div>
      </div>
    <% end %>
    <!-- /.modal-content -->
  </div>
  <!-- /.modal-dialog -->
</div>
<!-- /.modal -->
<div class="modal fade" id="cat_postfinish_popup" tabindex="-1" role="basic" aria-hidden="true">
  <div class="modal-dialog">
    <div class="modal-content c_modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-hidden="true"></button>
        <h4 class="modal-title"><%= cattype %></h4>
      </div>
      <div class="modal-body text-center">
        <div id="success"></div>
      </div>
      <div class="modal-footer c_modal-footer">
        <%= link_to  "Ok", cockpits_path, :class => "btn btn-success", :target => "_self" %>
      </div>
    </div>
    <!-- /.modal-content -->
  </div>
  <!-- /.modal-dialog -->
</div>
