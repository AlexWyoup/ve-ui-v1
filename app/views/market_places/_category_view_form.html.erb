<div class="widget-box">
	<div class="widget-title">
		<nav class="navbar navbar-default" role="navigation">
			<div class="container-fluid">
				<!-- Brand and toggle get grouped for better mobile display -->
				<div class="navbar-header">
					<button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1">
						<span class="sr-only">Toggle navigation</span>
						<span class="fa fa-bar"></span>
						<span class="fa fa-bar"></span>
						<span class="fa fa-bar"></span>
					</button>
				</div>
				<!-- Collect the nav links, forms, and other content for toggling -->
				<div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
					<div class="nav navbar-nav" id="filters">
						<br/>
						<select class="option-set clearfix"  data-filter-group="category">
							<option value="*" data-filter-value="" class="selected">All</option>
							<% categories.each do |cat| %>
							<option value="#filter-category-<%= cat %>" data-filter-value=".<%= cat %>"><%= cat %></option>
							<% end %>
						</select>
					</div>
					<form class="navbar-form navbar-right" role="search">
						<div class="form-group">
							<input type="text" class="form-control" placeholder="Search">
						</div>
						<button type="submit" class="btn btn-default">
							Submit
						</button>
					</form>
				</div><!-- /.navbar-collapse -->
			</div><!-- /.container-fluid -->
		</nav>
	</div>
	<div class="widget-content">
		<br/>
		</br/>
		<div class="row">
			<div class="col-lg-12">
				<br/>
				<div id="iso_container">
					<div class="gallery-masonry" >
						<% mkp_collection.each do |mkp| %>
						<div class="item produkt-element  <%= mkp.catagory %>" id='<%= mkp.catagory %>' >
							<a href="#" class="thumbnail" > <%= image_tag mkp.logo, :alt => mkp.name %> </a>
							<p>
								<%= mkp.catagory %>_<%= mkp.name %>
							</p>
							<div class="actions">
								<div class="actions-inner">
									<%= link_to  market_place_app_show_path(:name => mkp.name, :logo => mkp.logo, :features => mkp.features, :pricetype => mkp.pricetype, :plan => mkp.plan, :catagory => mkp.catagory, :attach => mkp.attach, :approved => mkp.approved), :target => "_self", title: "Open" do %><i class="fa fa-folder-open"></i><%end%>
								</div>
							</div>
						</div>
						<% end %>
					</div>
				</div>
			</div>
		</div>
	</div>
</div>