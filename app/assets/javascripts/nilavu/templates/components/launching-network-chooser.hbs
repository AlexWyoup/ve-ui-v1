<div class="row">
    <h3>{{i18n 'launcher.select_networking'}}</h3>
    <div class="bottom-divider"></div>
</div>
<div class="row pad_t25">
    <div class="col-md-6  pad_0">
        {{d-checkbox label="launcher.enable_ipv6" checked=enableIPv6Flag change="enableIPv6"}}
    </div>
    <div class="col-md-6  pad_0">
        {{d-checkbox label="launcher.enable_private_networking" checked=enablePrivNetworkFlag change="enablePrivNetwork"}}
    </div>
</div>
