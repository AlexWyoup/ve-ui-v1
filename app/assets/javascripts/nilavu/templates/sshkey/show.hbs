<div class="rt-box col-xs-12 col-sm-9 col-md-10 c_bg_white c_height_5">
    {{topic-list-header currentUser=currentUser title=title nameEnabled=nameEnabled name=name subTitleEnabled=subTitleEnabled subTitle=subTitle}}
    <div class="bottom-divider"></div>
    <div class="row">
        <div class="col-xs-6 col-sm-7 col-md-9 col-lg-9 col-xl-9">
            <h4>Activity</h4>
        </div>
        <div class="col-xs-6 col-sm-5 col-md-3 col-lg-3 col-xl-3">
            <ul class="list-inline c_sett_inline">
                <li>
                    <i class="c_icon-compose"></i><a id="create-link" {{action "sshCreate"}}>Create</a>
                </li>
                <li class="pull-right">
                    <i class="c_icon-import pull"></i><a id="import-link" {{action "sshImport"}}>Import</a>
                </li>
            </ul>
        </div>
    </div>
    {{#each model as |h|}}
        {{sshkey-download h=h}}
    {{/each}}

</div>
