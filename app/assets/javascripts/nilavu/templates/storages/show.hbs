<div class="rt-box col-xs-12 c_height_5">
    <div class="col-md-12">
        <button {{action 'upload'}} class='btn blue pull-right'>+upload files</button>
    </div>
    {{topic-list-header currentUser=currentUser title=title nameEnabled=nameEnabled name=name subTitleEnabled=subTitleEnabled subTitle=subTitle}}
    <div class="bottom-divider"></div>
    <div class="col-xs-12 col-sm-9 col-md-9 br_grey">
        <div class="row">
            <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12 col-xl-12">
                <table class="table yfiles-tb tb_upload objectPageList">
                    <tbody>
                        {{#each bucketfiles.message.usage as |fs|}}
                            {{bucket-file-item model=fs}}
                        {{/each}}
                    </tbody>
                </table>
            </div>
        </div>
    </div>
    {{bucket-files-usage bucketfiles=bucketfiles}}
</div>
