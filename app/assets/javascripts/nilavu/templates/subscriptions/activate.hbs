<section id="presignup" class="collection">
<div class="rt-box col-lg-12 col-sm-9 col-md-10 c_bg_white c_height_5">
  <h1 class="section-title">Select your account <span class=
      "main-color">model</span></h1>
  <br>
<section class="vm section-title">
  <div class ="row">
    <div class="col-md-8 pull-right">
    <div class="btn">
        {{sub-activate-tab panels=panels selectedTab=selectedTab tab="hourly"}}
      </div>
        <div class="btn">
        {{sub-activate-tab panels=panels selectedTab=selectedTab tab="monthly"}}
      </div>
  </div>
</div>
    <div class="sub-modal-body">
        {{#each panels as |tab|}}
            {{component tab selectedTab=selectedTab model=model subRegionOption=subRegionOption regions=regions}}
        {{/each}}
  </div>
</section>
</div>
</section>



